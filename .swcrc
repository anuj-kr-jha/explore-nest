{
  "$schema": "https://json.schemastore.org/swcrc",
  "sourceMaps": true,
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true
    },
    "baseUrl": "./",
    "target": "esnext",
    "preserveAllComments": false,
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "minify": false,
  "module": { "type": "es6" }
}
